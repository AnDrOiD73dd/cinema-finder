<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- regular -->
    <font
        android:font="@font/product_sans_regular"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/product_sans_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- bold -->
    <font
        android:font="@font/product_sans_bold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/product_sans_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />
    <font
        android:font="@font/product_sans_italic"
        android:fontStyle="italic"
        android:fontWeight="700"

        app:font="@font/product_sans_italic"
        app:fontStyle="italic"
        app:fontWeight="700" />
</font-family>